{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "windows": [
    "main"
  ],
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "opener:default",
    "deep-link:default",
    "autostart:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://cognivibe-server.vercel.app" },
        { "url": "https://server.cognivibe.tech" }
      ]
    }
  ]
}